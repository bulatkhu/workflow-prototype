profiles {
    standard {
        process {
            executor = 'local'
            cpus = 4
            memory = '5 GB'
            container = 'nvcr.io/nvidia/pytorch:xx' // override per process
        }
    }
    slurm {
        process.executor = 'slurm'
        process.queue = 'short'
    }
}


process {
    withLabel: msfragger {
        container = 'fcyucn/fragpipe:latest'
    }
}